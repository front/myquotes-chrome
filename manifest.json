{
  "name": "MyQuotes",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A fun way to save your quotes and snippets.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://api.flickr.com/",
    "contextMenus"
  ],
  "icons": {
    "16": "icon-bitty.png",
    "48": "icon-small.png",
    "128": "icon.png"
  }
}
